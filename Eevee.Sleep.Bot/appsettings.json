{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5913"
      }
    }
  },
  "AllowedHosts": "*",
  "AllowedOrigin": "<CORS_ORIGIN>",
  "Api": {
    "Internal": {
      "Endpoints": {
        "GenerateActivation": "<URL>"
      },
      "Token": {
        "Inbound": "<TOKEN>",
        "Outbound": "<TOKEN>",
        "Chester": "<TOKEN>"
      }
    }
  },
  "Discord": {
    "Token": "<TOKEN>",
    "Guild": "<GUILD_ID>",
    "Channels": {
      "AdminAlert": 0,
      "OfficialSiteAnnouncementNotification": {
        "JP": 0,
        "EN": 0,
        "ZH": 0
      },
      "InGameAnnouncementNotification": {
        "JP": 0,
        "EN": 0,
        "ZH": 0
      }
    },
    "Roles": {
      "Subscriber": 0,
      "AnnouncementNotification": {
        "JP": 0,
        "EN": 0,
        "ZH": 0
      }
    },
    "GameAnnouncement": {
      "ProxyUrlTemplate": "http://localhost:3000/proxy/announcement/in-game/{id}"
    }
  },
  "Mongo": {
    "Url": "mongodb://localhost:23015/"
  }
}
